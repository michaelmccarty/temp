{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": ["plugin:@typescript-eslint/eslint-recommended", "eslint:recommended" , "airbnb", "prettier"],
    "plugins": ["prettier", "@typescript-eslint"],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "project": "./tsconfig.json",
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": ["error"]
    }
}